        "code": "<Carousel><CarouselContent className=\"-ml-1\"><CarouselItem className=\"pl-1 md:basis-1/2\">Responsive Item</CarouselItem></CarouselContent><CarouselPrevious /><CarouselNext /></Carousel>"
