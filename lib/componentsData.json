{
  "alert": {
    "name": "alert",
    "type": "registry:ui",
    "description": "Displays a alert or a component that looks like a alert",
    "sampleCode": "<Alert size=\"lg\">\n  <Icon path={mdiInformationOutline} />\n  Solid lg\n</Alert>",
    "imageSrc": "/Blok-site-Illustration.png",
    "installation": [
      {
        "label": "npm",
        "code": "npx next-forge@latest init"
      },
      {
        "label": "yarn",
        "code": "yarn next-forge@latest init"
      },
      {
        "label": "pnpm",
        "code": "pnpm next-forge@latest init"
      },
      {
        "label": "bun",
        "code": "bunx next-forge@latest init"
      }
    ],
    "usage": {
      "import": "import { Button } from '@com'",
      "use": "<Button>this is button</Button>"
    },
    "examples": [
      {
        "type": "primary",
        "code": "<Alert variant=\"primary\"><AlertTitle>Primary Alert</AlertTitle><AlertDescription>This is a default (primary) alert. No title, only description.</AlertDescription></Alert>"
      },
      {
        "type": "danger",
        "code": "<Alert variant=\"danger\"><AlertTitle>Danger Alert</AlertTitle><AlertDescription> This is a danger alert with a title and description.</AlertDescription></Alert>"
      },
      {
        "type": "warning",
        "code": "<Alert variant=\"warning\">\n  <AlertTitle>Warning Alert</AlertTitle>\n  <AlertDescription>\n    This is a warning alert with a title and description.\n  </AlertDescription>\n</Alert>"
      },
      {
        "type": "success",
        "code": "<Alert variant=\"success\">\n  <AlertTitle>Success Alert</AlertTitle>\n  <AlertDescription>\n    This is a success alert with a title and description.\n  </AlertDescription>\n</Alert>"
      },
      {
        "type": "closable alert",
        "code": "<Alert variant=\"primary\"><AlertTitle>Closable Alert</AlertTitle>\n  <AlertDescription>\n    This is a primary alert with a title and description and even a close button.\n  </AlertDescription>\n  <Button\n    size=\"sm\"\n    variant=\"link\"\n    className=\"absolute top-2.5 right-3 h-6 shadow-none\"\n  >\n    Click\n  </Button>\n</Alert>"
      },
      {
        "type": "warning",
        "code": "<Alert variant=\"warning\"><AlertTitle>Warning Alert</AlertTitle><AlertDescription>This is a warning alert with a title and description.</AlertDescription></Alert>"
      }
    ],
    "dependencies": [
      "@radix-ui/react-slot"
    ],
    "files": [
      {
        "path": "registry/new-york/ui/alert.tsx",
        "type": "registry:ui"
      }
    ]
  },
