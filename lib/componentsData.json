    "imageSrc": "/Blok-site-Illustration.png",
    "installation": [
      {
        "label": "npm",
        "code": "npm install @radix-ui/react-select"
      },
      {
        "label": "yarn",
        "code": "yarn add @radix-ui/react-select"
      },
      {
        "label": "pnpm",
        "code": "pnpm add @radix-ui/react-select"
      }
    ],
    "usage": {
      "import": "import { Select, SelectContent, SelectItem, SelectTrigger, SelectValue } from '@/registry/new-york/ui/select'",
      "use": "<Select><SelectTrigger><SelectValue placeholder=\"Select\" /></SelectTrigger><SelectContent><SelectItem value=\"option1\">Option 1</SelectItem></SelectContent></Select>"
    },
    "examples": [
      {
        "type": "basic",
        "code": "<Select><SelectTrigger><SelectValue placeholder=\"Select a fruit\" /></SelectTrigger><SelectContent><SelectItem value=\"apple\">Apple</SelectItem></SelectContent></Select>"
      },
      {
        "type": "large-list",
        "code": "<Select><SelectTrigger><SelectValue placeholder=\"Large List\" /></SelectTrigger><SelectContent>{Array.from({ length: 100 }).map((_, i) => <SelectItem key={i} value={`item-${i}`}>Item {i}</SelectItem>)}</SelectContent></Select>"
      },
      {
        "type": "with-icons",
        "code": "<Select><SelectTrigger><SelectValue placeholder={<Icon path={mdiCircleOutline} />} /></SelectTrigger><SelectContent><SelectItem value=\"line\"><Icon path={mdiChartLine} />Line</SelectItem></SelectContent></Select>"
      }
    ],
    "dependencies": [
      "@radix-ui/react-select"
    ],
    "files": [
      {
        "path": "registry/new-york/ui/select.tsx",
        "type": "registry:ui"
      }
    ]
