    "imageSrc": "/Blok-site-Illustration.png",
    "installation": [
      {
        "label": "npm",
        "code": "npm install embla-carousel-react @radix-ui/react-slot"
      },
      {
        "label": "yarn",
        "code": "yarn add embla-carousel-react @radix-ui/react-slot"
      },
      {
        "label": "pnpm",
        "code": "pnpm add embla-carousel-react @radix-ui/react-slot"
      }
    ],
    "usage": {
      "import": "import { Carousel, CarouselContent, CarouselItem, CarouselNext, CarouselPrevious } from '@/registry/new-york/ui/carousel'",
      "use": "<Carousel><CarouselContent><CarouselItem>Content</CarouselItem></CarouselContent><CarouselPrevious /><CarouselNext /></Carousel>"
    },
    "examples": [
      {
        "type": "basic",
