        "type": "multiple",
        "code": "<Carousel opts={{align: 'start'}}><CarouselContent><CarouselItem className=\"md:basis-1/2\">Multiple Items</CarouselItem></CarouselContent><CarouselPrevious /><CarouselNext /></Carousel>"
